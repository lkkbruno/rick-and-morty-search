{
  "compilerOptions": {
    "outDir": "./dist/",
    "target": "es5",
    "lib": ["es6", "dom"],
    "jsx": "react",
    "module": "esnext",
    "moduleResolution": "node",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
  }
}
